<?xml version="1.0"?>

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <connectionStrings xdt:Transform="Replace">
    <!--Azure app service must contain encrypted connection strings-->
  </connectionStrings>
  <appSettings>
    <!--Azure app service must contain encrypted AppSettings-->
    <add key="BotUrl" xdt:Transform="Remove" xdt:Locator="Match(key)"/>
    <add key="BotName" xdt:Transform="Remove" xdt:Locator="Match(key)"/>
    <add key="BotKey" xdt:Transform="Remove" xdt:Locator="Match(key)"/>
  </appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
</configuration>
